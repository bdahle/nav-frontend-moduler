{
  "name": "@navikt/ds-react-internal",
  "version": "0.1.0",
  "private": false,
  "description": "NAV designsystem react components for internal sites",
  "author": "NAV Designsystem team",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "javascript",
    "internal"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/nav-frontend-moduler/tree/master/%40navikt/ds-react"
  },
  "sideEffects": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/cjs",
    "/esm",
    "/src"
  ],
  "scripts": {
    "create-package-json-pointers-to-esm": "node ../../../utilities/scripts/createPackageJsonsWithESMPointers.js",
    "clean": "rimraf cjs esm",
    "build": "yarn run clean && tsc -p ../tsconfig.json && tsc -p ../tsconfig-esm.json && yarn create-package-json-pointers-to-esm",
    "build-new": "yarn run clean && tsc -p ../tsconfig.json && tsc -p ../tsconfig-esm.json && yarn create-package-json-pointers-to-esm",
    "watch": "tsc --watch -p tsconfig-esm.json"
  },
  "dependencies": {
    "@navikt/ds-react": "^0.7.0",
    "react-merge-refs": "^1.1.0",
    "@navikt/ds-icons": "^0.5.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "3.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  }
}
